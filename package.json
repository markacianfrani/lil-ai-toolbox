{
  "name": "@cianfrani/lil-ai-toolbox",
  "version": "0.0.6",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./ai": {
      "types": "./dist/ai.d.ts",
      "import": "./dist/ai.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format src",
    "format:write": "biome format --write src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build && npm run typecheck && npm run lint",
    "publish": "npm run build && npm publish --access public"
  },
  "keywords": [
    "ai",
    "sdk",
    "tools",
    "agentic",
    "filesystem",
    "search",
    "typescript"
  ],
  "author": "Mark Anthony Cianfrani",
  "license": "ISC",
  "description": "A collection of standalone tools for agentic search and AI SDK integration",
  "dependencies": {
    "@ai-sdk/openai": "^2.0.52",
    "@ai-sdk/openai-compatible": "^1.0.22",
    "@cianfrani/lmstudio": "^0.0.7",
    "glob": "^10.3.10",
    "html-to-text": "^9.0.5",
    "minimatch": "^9.0.3",
    "turndown": "^7.1.2"
  },
  "optionalDependencies": {
    "ai": "^5.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/bun": "^1.3.0",
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^20.10.0",
    "@types/turndown": "^5.0.5",
    "ai": "^5.0.0",
    "typescript": "^5.3.3",
    "zod": "^4.1.12"
  }
}
